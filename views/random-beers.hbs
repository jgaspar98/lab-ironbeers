<h1>Random Beers</h1>
    {{#each randomBeers}}
    <div class="row">  
        <img class="bottles" src={{image_url}}/>
        <div class="collum randomInfo">  
            <h2>{{name}}</h2>
            <p>{{description}}</p>
            <ul>
                <h4>Food Pairing</h4>
                {{#each food_pairing}}
                    <li>{{this}}</li>
                {{/each}}
            </ul>
            <p>Brewers Tips:{{brewers_tips}}</p>
        </div>  
    </div>
    {{/each}} 
</div>